chiq <-function(x){
t <- table(x[,1],x[,2])
t <- addmargins(t)
m <- length(t[1,])
r <- length(t[,1])
ergebnis <- 0
for(j in 1:m){
for(k in 1:r){
njk <- (t[j,10]*t[10,k])/t[m,r]
ergebnis <- ergebnis + ((t[j,k] - njk)^2/t[m,r])
}
}
return(ergebnis)
}
